import matplotlib.pyplot as plt

# Sample data for sewage levels in a city
hours = [1, 2, 3, 4, 5, 6, 7, 8]
sewage_levels = [10, 12, 15, 11, 14, 16, 20, 18]

plt.plot(hours, sewage_levels)
plt.title('Sewage Levels Over Time')
plt.xlabel('Time (hours)')
plt.ylabel('Sewage Level')
plt.grid(True)
plt.show()
